  "Logging": { //All Logger providers
    "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": { //Valid only for the console
        "LogLevel": {
            "Services": "Information",
            "AppWebApi.Controllers": "Information"
        }
    }
  },

  //Move below to User secrets
  "AllowedHosts": "*"
}
